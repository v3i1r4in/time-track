datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgrespw@mercury.local:5670/timetrack?schema=public&connection_timeout=0&pool_timeout=0"
}

generator client {
  provider = "prisma-client-js"
}

model TimeBlock {
  id              Int    @id @default(autoincrement())
  startDateTime   Int
  endDateTime     Int
  spentOn         String
}

model Timer {
  name       String @id
  start      Int?
  due        Int?
  isActive   Boolean
  activity   String @default("")
}